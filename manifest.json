{
    "manifest_version": 2,
    "name": "Iris Plugin",
    "description": "This extension will do stuff",
    "version": "1.0",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "index.html"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["jquery-3.3.1.js", "app.js"]
        }
    ]
}